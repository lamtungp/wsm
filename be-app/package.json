{
    "name": "nodejs-typescript",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run db:migrate && nodemon -L src/index.ts",
        "db:migrate": "npm run build:migrations && sequelize db:migrate",
        "build:migrations": "tsc -p tsconfig.migrations.json",
        "migration:generate": "sequelize migration:generate --migrations-path src/migrations --name",
        "build": "tsc"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "jwt-decode": "^3.1.2",
        "jwt-simple": "^0.5.6",
        "morgan": "~1.9.1",
        "mysql2": "^2.2.5",
        "nodemailer": "^6.6.2",
        "reflect-metadata": "^0.1.13",
        "sequelize-typescript": "^2.1.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.11",
        "@types/express": "^4.17.12",
        "@types/http-errors": "^1.8.0",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/morgan": "^1.9.2",
        "@types/node": "^15.12.5",
        "@types/passport": "^1.0.6",
        "dotenv": "^10.0.0",
        "nodemon": "^2.0.7",
        "passport-jwt": "^4.0.0",
        "sequelize": "^6.6.4",
        "sequelize-cli": "^6.2.0",
        "ts-node": "^10.0.0",
        "typescript": "^3.9.7"
    },
    "main": ".prettierrc.js",
    "author": "",
    "license": "ISC",
    "description": ""
}
