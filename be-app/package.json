{
  "name": "nodejs-typescript",
  "version": "0.0.0",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "npm run db:migrate && nodemon -L src/index.ts",
    "db:migrate": "npm run build:migrations && sequelize db:migrate --migrations-path src/migrations",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "migration:generate": "sequelize migration:generate --migrations-path src/migrations --name",
    "build": "tsc",
    "test": "nodemon . --ext ts --exec \"mocha -w -r ts-node/register test/e2e/user/user.test.ts\"",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate --migrations-path src/migrations",
    "db:create:test": "cross-env NODE_ENV=test sequelize db:create",
    "test:unit": "nodemon . --ext ts --exec \"mocha -w -r ts-node/register test/unit/**/*.test.ts\"",
    "coverage": "\"nyc --reporter=lcov --reporter=text-summary mocha -r ts-node/register test/unit/**/*.test.ts\""
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "camelcase-keys": "^7.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validation": "^3.0.8",
    "express-winston": "^4.1.0",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.5.0",
    "jade": "~1.11.0",
    "mocha": "^9.0.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.6.2",
    "passport": "^0.4.1",
    "reflect-metadata": "^0.1.13",
    "sequelize-typescript": "^2.1.0",
    "sinon": "^11.1.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.11",
    "@types/express": "^4.17.12",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.5",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.6",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "chai": "^3.5.0",
    "chai-http": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "faker": "^5.5.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "passport-jwt": "^4.0.0",
    "prettier": "2.3.2",
    "sequelize": "^6.6.4",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.4",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.0.0",
    "typescript": "^3.9.7"
  },
  "main": ".prettierrc.js",
  "author": "",
  "license": "ISC",
  "description": ""
}
